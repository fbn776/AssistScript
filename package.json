{
	"name": "assistscript",
	"version": "1.2.9",
	"description": "AssistScript is a custom command based language",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"docs"
	],
	"bin": {
		"aslangc": "dist/cli.js"
	},
	"scripts": {
		"dev:repl:ts-node": "ts-node sandbox/repl.ts",
		"dev:repl:bun": "bun sandbox/repl.ts",
		"dev:ts-node": "ts-node sandbox/main.ts",
		"dev:bun": "bun sandbox/main.ts",
		"test": "jest",
		"build": "tsup",
		"start": "npm run build && node build/index.js",
		"gen-docs": "bun dev-tools/docs-gen/index.ts"
	},
	"keywords": [
		"language",
		"ts",
		"command",
		"interpreter"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fbn776/AssistScript.git"
	},
	"author": "fbn776",
	"url": "https://febinnelson.me",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@types/node": "^20.10.6",
		"jest": "^29.7.0",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"tsup": "^8.0.2",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"commander": "^12.0.0"
	}
}
