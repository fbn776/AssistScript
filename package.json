{
	"name": "assistscript",
	"version": "1.4.0",
	"description": "AssistScript is a custom command language",
	"main": "build/index.js",
	"scripts": {
		"dev:repl": "ts-node ./src/repl.ts",
		"dev:listen": "npx nodemon",
		"dev:main": "npx ts-node ./src/index.ts",
		"dev:debug": "node --inspect --require ts-node/register ./src/index.ts",
		"build": "rimraf ./build && tsc",
		"start": "npm run build && node build/index.js"
	},
	"keywords": [
		"language",
		"ts",
		"command",
		"interpreter"
	],
	"author": "fbn776",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "^20.10.6",
		"nodemon": "^3.0.2",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	}
}
